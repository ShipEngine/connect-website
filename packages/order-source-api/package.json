{
  "name": "@shipengine/connect-order-source-api",
  "version": "2.4.0",
  "description": "This is the typescript/javascript definitions for the order source api",
  "homepage": "https://github.com/ShipEngine/connect",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "bump": "bump $npm_confg_release_type --commit 'release order-source-api v' --tag 'order-source-api_v' --push --all",
    "clean": "shx rm -rf .nyc_output coverage lib tsconfig.tsbuildinfo",
    "dist-clean": "yarn clean && rm -rf node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\"",
    "release": "yarn clean && yarn build && yarn test && yarn bump",
    "test": "true",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/connect"
  },
  "keywords": [
    "shipengine",
    "connect",
    "ordersourceapi"
  ],
  "author": "Justin Robertson",
  "license": "Apache-2.0",
  "devDependencies": {
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  }
}
