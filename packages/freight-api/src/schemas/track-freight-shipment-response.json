{
  "title": "TrackFreightShipmentResponse",
  "type": "object",
  "required": [
    "events",
    "pro_number",
    "scac",
    "transaction_id",
    "transit"
  ],
  "properties": {
    "transaction_id": {
      "type": "string"
    },
    "scac": {
      "type": "string",
      "description": "The Standard Carrier Alpha Code developed by the NMFTA is used to identify a freight carrier within the transportation industry."
    },
    "pro_number": {
      "type": "string"
    },
    "ship_from": {
      "type": "object",
      "properties": {
        "address": {
          "type": "object",
          "properties": {
            "postal_code": {
              "type": "string"
            }
          }
        }
      }
    },
    "ship_to": {
      "type": "object",
      "properties": {
        "address": {
          "type": "object",
          "properties": {
            "postal_code": {
              "type": "string"
            }
          }
        }
      }
    },
    "transit": {
      "type": "object",
      "properties": {
        "pickup": {
          "type": "object",
          "required": [
            "date"
          ],
          "properties": {
            "date": {
              "type": "string",
              "format": "date",
              "description": "Date Format: YYYY-MM-DD"
            }
          }
        },
        "delivery": {
          "type": "object",
          "properties": {
            "estimated": {
              "type": "object",
              "required": [
                "date"
              ],
              "properties": {
                "date": {
                  "type": "string",
                  "format": "date",
                  "description": "Date Format: YYYY-MM-DD"
                },
                "time": {
                  "type": "string",
                  "format": "time",
                  "description": "24 Hour Format: HH:MM:SS, HH:MM:SSZ, HH:MM:SS+/-HH:MM"
                }
              }
            },
            "actual": {
              "type": "object",
              "required": [
                "date"
              ],
              "properties": {
                "date": {
                  "type": "string",
                  "format": "date",
                  "description": "Date Format: YYYY-MM-DD"
                },
                "time": {
                  "type": "string",
                  "format": "time",
                  "description": "24 Hour Format: HH:MM:SS, HH:MM:SSZ, HH:MM:SS+/-HH:MM"
                }
              }
            },
            "signature": {
              "type": "object",
              "required": [
                "name"
              ],
              "properties": {
                "name": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "events": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "status",
          "date"
        ],
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "delivered",
              "exception",
              "in_transit",
              "info",
              "out_for_delivery",
              "picked_up",
              "unknown"
            ],
            "description": "Valid values for tracking status codes include:\n  * `delivered` - The shipment has been delivered\n  * `exception` - Denotes that an exception occurred that is outside of the normal shipment lifecycle\n  * `in_transit` - The shipment is in transit\n  * `info` - Informational status about the shipment\n  * `out_for_delivery`: The shipment is out for delivery\n  * `picked_up`: The shipment has been picked up\n  * `unknown`: Used to indicate statuses that may be undocumented by a carrier.\n"
          },
          "date": {
            "type": "string",
            "format": "date",
            "description": "Date Format: YYYY-MM-DD"
          },
          "time": {
            "type": "string",
            "format": "time",
            "description": "24 Hour Format: HH:MM:SS, HH:MM:SSZ, HH:MM:SS+/-HH:MM"
          },
          "location": {
            "type": "object",
            "properties": {
              "city_locality": {
                "type": "string"
              },
              "state_province": {
                "type": "string"
              }
            }
          },
          "carrier": {
            "type": "object",
            "properties": {
              "description": {
                "type": "string",
                "description": "Optional carrier-supplied description of the tracking event"
              }
            }
          }
        }
      }
    }
  }
}
