{
  "name": "@shipengine/connect-inventory-api",
  "version": "0.1.0",
  "description": "OpenAPI specification and TypeScript definitions for the Connect Inventory API",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "yarn build:ts",
    "format": "prettier --single-quote --trailing-comma=all --write \"src/**/*.ts\"",
    "lint": "prettier --single-quote --trailing-comma=all --check \"src/**/*.ts\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShipEngine/connect.git"
  },
  "keywords": [
    "shipengine",
    "connect",
    "inventoryapi"
  ],
  "author": "ShipEngine",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "jest": "^26.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.2.4"
  }
}
