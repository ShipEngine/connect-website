{
  "name": "@shipengine/connect-order-source-runtime",
  "version": "1.2.0",
  "description": "This is the app host for a order source Connect app. It provides an HTTP interface implementing the OrderSourceAPI spec and does mapping to Connect and back",
  "main": "lib/server.js",
  "scripts": {
    "build": "tsc --build",
    "bump": "bump $npm_confg_release_type --commit 'release order-source-runtime v' --tag 'order-source-runtime_v' --push --all",
    "clean": "shx rm -rf .nyc_output coverage lib tsconfig.tsbuildinfo",
    "dist-clean": "yarn clean && rm -rf node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\"",
    "release": "yarn clean && yarn build && yarn test && yarn bump",
    "start": "node lib/server.js",
    "test": "true",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/connect"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@sentry/node": "^5.26.0",
    "@shipengine/connect-loader": "^3.1.0",
    "@shipengine/connect-order-source-api": "^2.3.7",
    "@shipengine/connect-sdk": "^12.9.0",
    "body-parser": "1.19.0",
    "continuation-local-storage": "3.2.1",
    "dotenv-flow": "3.2.0",
    "express": "4.17.1",
    "express-winston": "4.0.3",
    "fast-redact": "2.0.0",
    "newrelic": "6.10.0",
    "only": "0.0.2",
    "redact-secrets": "1.0.0",
    "serialize-error": "^7.0.1",
    "uuid": "8.2.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/continuation-local-storage": "3.2.2",
    "@types/express": "4.17.6",
    "@types/fast-redact": "2.0.0",
    "@types/node": "^14.14.31",
    "@types/uuid": "8.0.0",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.18.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "typescript": "^4.1.3"
  }
}
