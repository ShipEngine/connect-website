{
	"name": "@shipengine/connect-shipping-runtime",
	"version": "1.0.2",
	"description": "This is the app host for a carrier Connect app. It provides an HTTP interface implementing the CAPI spec and does mapping CAPI -> Connect -> CAPI",
	"main": "lib/server.js",
	"scripts": {
		"build": "tsc --build",
		"bump": "bump patch --commit 'release shipping-runtime v' --tag 'shipping-runtime_v' --push --all",
		"clean": "shx rm -rf .nyc_output coverage lib tsconfig.tsbuildinfo",
		"dist-clean": "yarn clean && rm -rf node_modules",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "prettier --check \"src/**/*.ts\"",
		"release": "yarn clean && yarn build && yarn test && yarn bump",
		"start": "node lib/server.js",
		"test": "true",
		"watch": "tsc --watch"
	},
	"repository": {
		"type": "git",
		"url": "github.com/shipstation/integrations-shipping"
	},
	"author": "",
	"license": "UNLICENSED",
	"dependencies": {
		"@sentry/node": "^5.24.2",
		"@shipengine/connect-carrier-api": "^1.13.5",
		"@shipengine/connect-loader": "^3.2.0",
		"@shipengine/connect-sdk": "^12.6.4",
		"body-parser": "1.19.0",
		"continuation-local-storage": "3.2.1",
		"dotenv-flow": "3.2.0",
		"express": "4.17.1",
		"express-winston": "4.0.3",
		"fast-redact": "2.0.0",
		"newrelic": "6.10.0",
		"only": "0.0.2",
		"redact-secrets": "1.0.0",
		"serialize-error": "^8.0.1",
		"uuid": "8.2.0",
		"winston": "3.3.3"
	},
	"devDependencies": {
		"@types/continuation-local-storage": "3.2.2",
		"@types/express": "4.17.6",
		"@types/fast-redact": "2.0.0",
		"@types/hapi__joi": "^17.1.4",
		"@types/node": "^14.14.31",
		"@types/uuid": "8.0.0",
		"@typescript-eslint/eslint-plugin": "3.6.0",
		"@typescript-eslint/parser": "3.1.0",
		"eslint": "6.8.0",
		"eslint-config-google": "0.14.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-jest": "23.18.0",
		"jest": "26.1.0",
		"nodemon": "2.0.4",
		"prettier": "2.0.5",
		"typescript": "^4.1.3"
	}
}
