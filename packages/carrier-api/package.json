{
  "name": "@shipengine/connect-carrier-api",
  "version": "1.13.7",
  "description": "This is the typescript/javascript definitions for carrier api",
  "homepage": "https://github.com/ShipEngine/connect",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "bump": "bump $BUMP_RELEASE_TYPE --commit 'release carrier-api v' --tag 'carrier-api_v' --push --all",
    "clean": "shx rm -rf .nyc_output coverage lib tsconfig.tsbuildinfo",
    "dist-clean": "yarn clean && rm -rf node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\"",
    "release": "yarn clean && yarn build && yarn test && yarn bump",
    "test": "true",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/shipengine/connect"
  },
  "keywords": [
    "ipaas",
    "capi",
    "integrations-shipping"
  ],
  "author": "Justin Robertson",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^4.0.5"
  }
}
