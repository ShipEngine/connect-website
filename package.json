{
  "name": "@shipengine/ipaas-loader",
  "version": "0.0.9",
  "description": "Loads a ShipEngine IPaaS app from an NPM package",
  "keywords": [
    "shipengine",
    "ipaas"
  ],
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://www.shipengine.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://www.shipengine.com/docs/ipaas/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/shipengine-ipaas-loader.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true,
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.3",
    "@jsdevtools/tslint-modular": "^2.0.5",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@types/chai": "^4.1.7",
    "@types/js-yaml": "^3.12.3",
    "@types/json5": "0.0.30",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/source-map-support": "^0.5.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.1",
    "shx": "^0.3.2",
    "tslint": "^6.1.1",
    "typescript-tslint-plugin": "^0.5.0"
  },
  "dependencies": {
    "@jsdevtools/humanize-anything": "^1.1.0",
    "@jsdevtools/ono": "^7.1.1",
    "@shipengine/ipaas": "0.0.11",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.3",
    "resolve-from": "^5.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typescript": "^3.5.1"
  }
}
