{
  "name": "@shipengine/connect-local-dev-ui",
  "version": "2.0.7",
  "description": "The ShipEngine Integration Platform local development UI",
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://connect.shipengine.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://connect.shipengine.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShipEngine/connect-local-dev-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ShipEngine/connect-local-dev-ui/issues"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "react-scripts build && tsc -p ./scripts/post-build/tsconfig.json && node ./scripts/remove-deps.js",
    "bundlesize": "bundlesize --config ./bundlesize.config.js",
    "bump": "bump --tag --push --all",
    "clean": "shx rm -rf coverage build",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint src/**/*.ts{,x}",
    "release": "npm run upgrade && npm run clean && npm run lint && CI=true react-scripts test && npm run bump",
    "prestart": "react-env --env /path/to/.env",
    "start": "PORT=4000 react-scripts start",
    "test": "react-scripts test",
    "upgrade": "npm-check -u && npm audit fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@beam-australia/react-env": "^2.1.2",
    "@rjsf/antd": "^2.4.0",
    "@rjsf/core": "^2.4.0",
    "@shipengine/connect-sdk": "^6.0.0",
    "antd": "^4.6.5",
    "axios": "^0.20.0",
    "dayjs": "^1.8.36",
    "json-schema": "^0.2.5",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-pretty": "^2.2.0",
    "react-query": "^2.23.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/eslint": "^7.2.2",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-jsonschema-form": "^1.7.4",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-syntax-highlighter": "^11.0.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "bundlesize": "^0.18.0",
    "eslint-plugin-jest": "^24.0.2",
    "npm-check": "^5.9.2",
    "react-query-devtools": "^2.5.0",
    "shx": "^0.3.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
